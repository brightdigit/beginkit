        {{#each collections.posts}}
            {{#if this.posts}}
                {{#each this.posts}}
                    {{this.content}}
                    {{#each this.profiles as |dates name|}}
                        {{name}}
                            {{#each dates}}                        
                                {{this}}
                            {{/each}}
                    {{/each}}
                {{/each}}
            {{/if}}
        {{/each}}